@{
    ViewBag.Title = "Plan Semanal";
}
<div class="row">
    <div class="col-lg-12">
        <h4 class="page-header">
            Captura Plan Semanal
            <button class="btn btn-xs btn-outline btn-primary pull-right btnNuevo" ><i class="fa fa-file-o fa-lg fa-fw"></i> Nuevo Plan</button>
        </h4>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="well well-sm" style="background-color: #fff; border-color:#337AB7;">
    <div class="row">
        <div class="col-lg-3">
            <select id="selPeriodos" class="form-control">
                <option value="0"> Cargando...</option>
            </select>
        </div>
        <div class="col-lg-3">
            <a href="javascript:;" class="btn btn-primary" id="btnFilter"><i class="fa fa-filter"></i> Filtrar</a>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">Plan Semanal</h3>
            </div>
            <div class="panel-body">
                <div id="bbGrid-clear" style="overflow-x: auto; overflow-y: hidden;">
                    <div id="cargandoInfo">
                        <div class='alert alert-warning'>
                            <button type='button' class='close' data-dismiss='alert'>x</button>
                            Seleccionar periodo si desea filtrar la información
                        </div>
                    </div>
                </div>
                <br />
                <br />
                <br />
            </div>
        </div>
    </div>
</div>

<div class="modal fade bs-example-modal-lg" id="nuevo-PlanSemanal"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoPlanSemanalLabel">
</div>

<div class="modal fade bs-example-modal-lg" id="nuevo-PlanDetalle"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoPlanDetalleLabel">
</div>

<div class="modal fade bs-example-modal-lg" id="nuevo-Vehiculo"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoVehiculoLabel">
</div>

<div class="modal fade bs-example-modal-lg" id="actualiza-PlanDetalle"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoPlanDetalleLabel">
</div>


@section Scripts {
    @Html.IncludeVersionedJs(Url.Content("~/Scripts/PlanSemanal.js"))
}
