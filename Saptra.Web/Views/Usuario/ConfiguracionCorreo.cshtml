@{
    ViewBag.Title = @Resources.Global.menuConfiguracionCorreo;
}
<div class="row">
    <div class="col-lg-12">
        <h4 class="page-header">
            @Resources.Global.menuConfiguracionCorreo
            <button class="btn btn-xs btn-outline btn-primary pull-right btnNuevo" style="display: none;"><i class="fa fa-file-o fa-lg fa-fw"></i> Nuevo</button>
        </h4>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <div class="col-lg-12">
        <div id="bbGrid-clear" style="overflow-x: auto; overflow-y: auto;">
            <div id="cargandoInfo">@Resources.Global.menuLoading</div>
        </div>
        <br />
        <br />
        <br />
    </div>
</div>

<div class="modal fade bs-example-modal-lg" id="nuevo-Correo"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoUsuarioLabel">
</div>

<div class="modal fade bs-example-modal-lg" id="actualiza-Correo"
     tabindex="-1" role="dialog"
     aria-labelledby="nuevoUsuarioLabel">
</div>


@section Scripts {
    @Html.IncludeVersionedJs(Url.Content("~/Scripts/ConfigurarCorreo.js"))
}
