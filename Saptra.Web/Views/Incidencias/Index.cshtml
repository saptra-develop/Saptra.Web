@{
    ViewBag.Title = "Plan Semanal";
}
<div class="row">
    <div class="col-lg-12">
        <h4 class="page-header">
            Reporte de Incidencias
            <button class="btn btn-xs btn-outline btn-success pull-right btnExportar" style="margin-right:5px;"><i class="fa fa-file-o"></i> Exportar reporte</button>
        </h4>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="well well-sm" style="background-color: #fff; border-color:#337AB7;">
    <div class="row">
        <div class="col-lg-5">
            <select id="selPeriodos" class="js-example-basic-multiple js-states form-control" multiple="multiple"></select>
            <input type="hidden" id="selecPeriodos" />
        </div>
        <div class="col-lg-5">
            <select id="selTipoActividad" class="js-example-basic-multiple js-states form-control" multiple="multiple"></select>
            <input type="hidden" id="selecTipoActividad" />
        </div>
    </div>
    </br>
    <div class="row">
        <div class="col-lg-5">
            <select id="selTipoFigura" class="js-example-basic-multiple js-states form-control" multiple="multiple"></select>
            <input type="hidden" id="selecTipoFigura" />
        </div>
        <div class="col-lg-5">
            <select id="selNombreFigura" class="js-example-basic-multiple js-states form-control" multiple="multiple"></select>
            <input type="hidden" id="selecNombreFigura" />
        </div>
        <div class="col-lg-1">
            <a href="javascript:;" class="btn btn-primary" id="btnFilter"><i class="fa fa-filter"></i> Filtrar</a>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">Incidencias</h3>
            </div>
            <div class="panel-body">
                <div id="bbGrid-clear">
                    <div id="cargandoInfo">
                        <div class='alert alert-warning'>
                            <button type='button' class='close' data-dismiss='alert'>x</button>
                            Seleccionar filtros para obtener información
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    @Html.IncludeVersionedJs(Url.Content("~/Scripts/Incidencias.js"))
}
